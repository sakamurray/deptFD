   "source": [
    "import time\n",
    "\n",
    "def fib(n):\n",
    "    if n < 2:\n",
    "        return n\n",
    "    else:\n",
    "        return fib(n-2) + fib(n-1)\n",
    "\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "t1 = time.time()\n",
    "\n",
    "N = fib(n)\n",
    "\n",
    "t2 = time.time()\n",
    "\n",
    "print(N)\n",
    "\n",
    "elapsed_time = t2-t1\n",
    "print(f\"経過時間：{elapsed_time}\")"
   ]
