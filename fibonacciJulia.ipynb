   "source": [
    "function fib(n)\n",
    "    if n < 2\n",
    "        return n\n",
    "    else\n",
    "        return fib(n-2) + fib(n-1)\n",
    "    end\n",
    "end\n",
    "\n",
    "n = readline()\n",
    "n = parse(Int64, n)\n",
    "\n",
    "@time fib(n)\n",
    "\n",
